    <script>

        $("#form").validate({
            rules: {
                name: {
                    required: true,
                    lettersonly: true
                },
                email: {
                    required: true,
                    email: true
                },
                mobile: {
                    required: true,
                    digits: true,
                    minlength: 10
                },
                password: {
                    required: true,
                    minlength: 6
                },
                rePassword: {
                    equalTo: '#password'
                }
            },
            messages: {
                name: {
                    required: 'Enter your name',
                    lettersonly: 'Only characters allowed'
                },
                email: {
                    required: 'Enter your email',
                    email: 'Invalid email ID'
                },
                mobile: {
                    required: 'Enter your mobile number',
                    digits: 'Enter numbers only',
                    minlength: 'Less than 10 digits entered'
                },
                password: {
                    required: 'Enter a password',
                    minlength: 'Must have minimum of 6 characters'
                },
                rePassword: {
                    equalTo: 'Password not match'
                }
            }

        })


    </script>
    
